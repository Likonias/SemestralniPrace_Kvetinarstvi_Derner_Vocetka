<UserControl x:Class="SemestralniPrace_Kvetinarstvi_Derner_Vocetka.Components.NavigationBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SemestralniPrace_Kvetinarstvi_Derner_Vocetka.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="33*"/>
            <ColumnDefinition Width="7*"/>
        </Grid.ColumnDefinitions>
        <Button Content="Login" HorizontalAlignment="Left" Margin="29,15,0,0" VerticalAlignment="Top" Width="60" Command="{Binding NavigateLoginCommand}" Grid.Column="1" Height="20"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="94,15,0,0" VerticalAlignment="Top" Width="60" Command="{Binding NavigateRegisterCommand}" Grid.Column="1" Height="20"/>
        <ComboBox HorizontalAlignment="Left" Grid.Column="0" Margin="94,14,0,0" VerticalAlignment="Top" Width="120" Height="22"/>

    </Grid>
</UserControl>
